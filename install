if [ -z "${DEFAULTPATH}" ]; then
  DEFAULTPATH="${PATH}"
  export DEFAULTPATH
fi

PYENV="$HOME/Documents/iot_wand/env/bin"
export PYENV

py3="${PYENV}/python3"
export py3

pip3="${py3} -m pip"
export pip3

iot_wand="$HOME/Documents/iot_wand"
export iot_wand

SOURCEIOTWAND="$HOME/Documents:$HOME/Documents/iot_wand:$HOME/Documents/iot_wand/iot_wand"
export SOURCEIOTWAND

SOURCES="${SOURCEIOTWAND}"
export SOURCES

PYTHONPATH="${PYENV}:${SOURCES}"
export PYTHONPATH

PATH="${PYTHONPATH}:${DEFAULTPATH}"
export PATH
